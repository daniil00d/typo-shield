    "typo-shield-server": "^1.0.0",
    "yaml": "^2.4.2",
    "yamljs": "^0.3.0"
  },
  "devDependencies": {
    "@types/swagger-ui-express": "^4.1.6",
    "@types/yamljs": "^0.2.34",
    "antlr4ts": "^0.5.0-alpha.4",
    "vite": "^5.2.11",
    "vite-plugin-dts": "^3.9.1"
