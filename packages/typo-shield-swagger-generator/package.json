    "vite:build": "npm run vite:build:server && npm run vite:build:main",
    "start:test": "nodemon src/index.ts generate --folder examples",
    "start:prod": "node dist/index.js generate --folder examples",
    "swagger:serve": "npx typo-shield-server -f dist/swagger.yaml"
  },
