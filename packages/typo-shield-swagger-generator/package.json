{
  "name": "typo-shield-swagger-generator",
  "version": "2.0.6",
  "description": "",
  "main": "dist/index.js",
  "type": "commonjs",
  "bin": {
    "typo-shield-swagger-generator": "dist/index.js"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc --project tsconfig.json --outDir dist",
    "vite:build:server": "LIB_NAME=server vite build",
