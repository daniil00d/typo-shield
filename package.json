{
  "name": "typo-shield",
  "version": "0.0.0",
  "description": "Core for DSL language that constraints endpoints",
  "main": "dist/index.ts",
  "type": "commonjs",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "npx nodemon",
    "build": "tsc --project tsconfig.json && tscpaths -p tsconfig.json -s ./src -o ./dist",
    "start:production": "node ./dist/index.js",
    "antlr4ts": "antlr4ts -visitor src/antlr-ts-compiler/grammar/TypoShield.g4",
    "generate-cert": "sh ./scripts/generate-cert.sh"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/express": "^4.17.17",
    "@types/spdy": "^3.4.6",
    "antlr4ts-cli": "^0.5.0-alpha.4",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "jest": "^29.7.0",
    "tsconfig-paths": "^4.2.0",
    "tscpaths": "^0.0.9",
    "typescript": "^5.2.2"
  },
  "nodemonConfig": {
    "ignore": [
      ".git",
      "node_modules"
    ],
    "watch": [
      "src"
    ],
    "exec": "node -r tsconfig-paths/register -r ts-node/register ./src/index.ts",
    "ext": "ts, js"
  },
  "dependencies": {
    "antlr4": "^4.13.1",
    "antlr4ts": "^0.5.0-alpha.4",
    "spdy": "^4.0.2",
    "ts-node": "^10.9.1",
    "typo-shield-swagger-generator": "^1.0.6"
  },
  "directories": {
    "doc": "docs"
  }
}
