{
  "name": "typo-shield",
  "version": "0.0.0",
  "description": "Core for DSL language that constraints endpoints",
  "main": "dist/index.ts",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "npx nodemon",
    "build": "tsc -p .",
    "start:production": "node ./dist/index.js",
    "antlr4ts": "antlr4ts -visitor src/antlr-ts-compiler/grammar/langgrammar.g4"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/express": "^4.17.17",
    "antlr4ts-cli": "^0.5.0-alpha.4",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "jest": "^29.7.0",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.2.2"
  },
  "nodemonConfig": {
    "ignore": [
      ".git",
      "node_modules"
    ],
    "watch": [
      "src"
    ],
    "exec": "node -r tsconfig-paths/register -r ts-node/register ./src/antlr-ts-compiler/index.ts",
    "ext": "ts, js"
  },
  "dependencies": {
    "antlr4": "^4.13.1",
    "antlr4ts": "^0.5.0-alpha.4",
    "ts-node": "^10.9.1"
  }
}
